QCheckBox#BottomBarButton{
    border:none;
    color:rgb(255,255,255);
    background:transparent;
    border:none;
    padding:1px 4px;
}
QCheckBox#BottomBarButton::indicator {
    width: 0px;
    height: 0px;
}
QCheckBox#BottomBarButton:hover{
    background:rgb(23,83,152);
}
QCheckBox#BottomBarButton:checked{
