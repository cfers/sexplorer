/**  皮肤blue
标题栏底色                           rgb(11,66,96);
标题栏下边线 按钮pressed checked     rgb(11,39,72);
工具栏上边线 工具栏分割线             rgb(20,72,132);
文本颜色                            rgb(255,255,255);
文本提示                            rgb(160,160,160);
按钮hover                           rgb(23,83,152);
**/


/**********************通用设置 字体 背景色**************************************/
QWidget{
    font: 9pt "微软雅黑";
}

QLabel#label_version{
    color:white;
}


/*********************外边框********************/
QWidget#wid_back{
    border: 2px solid  rgb(11,66,96);
}

/*********************外边框********************/
QWidget#wid_pop_back{
    background-color: white;
    border: 2px solid  rgb(11,66,96);
}

/***********标题栏颜色***********************/
QWidget#wid_titleBar{
    background-color:  rgb(11,66,96);
    border-bottom:1px solid rgb(11,39,72);
}

/*************分割线********************************/
QFrame#frame_vline{
    margin:5px;
    color:rgb(20,72,132);
}
QFrame#frame_hline{
    margin:5px;
    color:rgb(20,72,132);
}

/******************菜单栏和菜单样式**************************/
QMenuBar{
    border:none;
    background-color: transparent;
}
QMenuBar::item {
    margin:3px;
    padding: 2px 5px;
    background: transparent;
    color:rgb(255,255,255);
    spacing: 3px;
}
QMenuBar::item:hover, QMenuBar::item:selected, QMenuBar::item:pressed{
    color: rgb(11,66,96);
    background: rgb(255,255,255);
}
/*QMenu{
    background-color:  rgb(11,66,96);
}
QMenu::item {
    background-color: transparent;
    color:rgb(255,255,255);
}
QMenu::item:selected {
    background-color:  rgb(11,66,96);
}*/


/***********************工具栏样式*****************************/
QToolBar {
    border:none;
    border-top:1px solid rgb(20,72,132);
    background-color:  rgb(11,66,96);
    padding: 5px;
    spacing: 3px;
}
QToolBar::handle {
    width:0px;
    height:0px;
}
QToolBar::separator{
    width:1px;
    margin:5px;
    background:rgb(20,72,132);
}

/****************************分割线**********************/
QSplitter::handle {
      background: rgb(11,66,96);
}



/***********************工具栏按钮*************************/
QPushButton#ToolBtn_Comment{
    image: url(:/images/btn_win_skin.png);
}

/***************************窗口按钮***********************/
QPushButton#btn_win_skin,QPushButton#btn_win_min,QPushButton#btn_win_max,QPushButton#btn_win_close{
    padding:2px 2px;
    background:transparent;
    border:none;
}
QPushButton#btn_win_skin{
    image: url(:/images/btn_win_skin.png);
}
QPushButton#btn_win_min{
    image: url(:/images/btn_win_min.png);
}
QPushButton#btn_win_max{
    image: url(:/images/btn_win_normal.png);
}
QPushButton#btn_win_close{
    image: url(:/images/btn_win_close.png);
}
QPushButton:hover#btn_win_skin,QPushButton:hover#btn_win_min,QPushButton:hover#btn_win_max{
    background: rgb(23,83,152);
}
QPushButton:hover#btn_win_close{
    background: red;
}

/*******************ActionButton**************************/
QPushButton#btn_action{
    background: rgb(200,200,200);
    color:black;
}
QPushButton:hover#btn_action{
    background: rgb(23,83,152);
    color:white;
}
QPushButton:pressed#btn_action{
    background: rgb(11,39,72);
    color:white;
}



